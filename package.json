{
  "name": "milligrami",
  "version": "0.2.3-1",
  "description": "Milligram React wrapper",
  "main": "lib/index.js",
  "typings": "types/index.d.ts",
  "scripts": {
    "test": "jest __test__",
    "build": "tsc -p ./",
    "rebuild": "npm run clean && npm run build",
    "clean": "rimraf ./lib",
    "bundle": "webpack --config ./config/webpack.config.js --mode production",
    "clean:bundle": "rimraf ./bundle",
    "rebundle": "npm run clean:bundle && npm run bundle",
    "build:all": "npm run rebuild && npm run rebundle"
  },
  "keywords": [
    "milligram",
    "react",
    "typescript"
  ],
  "author": "d10221",
  "license": "MIT",
  "dependencies": {
    "milligram": "^1.3.0"
  },
  "devDependencies": {
    "@types/jest": "22.2.0",
    "@types/jsdom": "^11.0.0",
    "@types/react": "16.0.40",
    "@types/react-dom": "16.0.4",
    "jest": "22.4.2",
    "jsdom": "^11.0.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "ts-jest": "22.4.2",
    "ts-loader": "4.1.0",
    "typescript": "2.7.2",
    "webpack": "4.1.1",
    "webpack-cli": "2.0.12"
  },
  "directories": {
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/D10221/milligrami.git"
  },
  "bugs": {
    "url": "https://github.com/D10221/milligrami/issues"
  },
  "homepage": "https://github.com/D10221/milligrami#readme",
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
