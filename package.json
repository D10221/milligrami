{
  "name": "milligrami",
  "version": "0.0.4-0",
  "description": "Milligram React wrapper",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "test": "mocha ./test.js",
    "build": "tsc -p ./",
    "clean": "rimraf ./built",
    "rebuild": "npm run clean && npm run build",
    "build:css": "node-sass ./src/__demo__ -o built/__demo__",
    "build:demo": "npm run clean && npm run build && npm run build:css && node ./scripts/pug.js",
    "start": "electron lib/__demo__/main.js",
    "build:lib": "rimraf lib && tsc -p tsconfig.lib.json"
  },
  "keywords": [
    "milligram",
    "react",
    "typescript"
  ],
  "author": "d10221",
  "license": "MIT",
  "dependencies": {
    "react": "^15.5.4",
    "milligram": "^1.3.0"
  },
  "devDependencies": {
    "@types/debug": "0.0.29",
    "@types/react": "^15.0.24",
    "@types/react-dom": "^15.5.0",
    "debug": "^2.6.8",
    "electron": "^1.6.10",
    "pug": "^2.0.0-rc.2",
    "react-dom": "^15.5.4",
    "typescript": "^2.3.4"
  },
  "directories": {
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/D10221/milligrami.git"
  },
  "bugs": {
    "url": "https://github.com/D10221/milligrami/issues"
  },
  "homepage": "https://github.com/D10221/milligrami#readme"
}
