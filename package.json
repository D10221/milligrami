{
  "name": "milligrami",
  "version": "0.1.1-0",
  "description": "Milligram React wrapper",
  "main": "lib/index.js",
  "typings": "types/index.d.ts",
  "scripts": {
    "test": "mocha built/__test__/*.test.js",
    "build": "tsc -p ./",
    "rebuild": "npm run clean && npm run build && npm run build:css && npm run build:html",
    "clean": "rimraf ./built",
    "build:css": "node-sass ./src/__demo__ -o built/__demo__",
    "build:html": "node ./scripts/pug-window.js",
    "build:lib": "tsc -p tsconfig.lib.json",
    "clean:lib": "rimraf ./lib",
    "rebuild:lib": "npm run clean:lib && npm run build:lib",
    "bundle": "webpack --config ./webpack.config.js",    
    "clean:bundle": "rimraf ./bundle",
    "rebundle": "npm run clean:bundle && npm run bundle",
    "build:all": "npm run rebuild && npm run rebuild:lib &&npm run rebundle"
  },
  "keywords": [
    "milligram",
    "react",
    "typescript"
  ],
  "author": "d10221",
  "license": "MIT",
  "dependencies": {
    "react": "^15.5.4",
    "milligram": "^1.3.0"
  },
  "devDependencies": {
    "@types/debug": "0.0.29",
    "@types/jsdom": "^11.0.0",
    "@types/mocha": "^2.2.41",
    "@types/react": "^15.0.24",
    "@types/react-dom": "^15.5.0",
    "awesome-typescript-loader": "^3.1.3",
    "debug": "^2.6.8",
    "electron": "^1.6.10",
    "jsdom": "^11.0.0",
    "node-sass": "^4.5.3",
    "pug": "^2.0.0-rc.2",
    "react-dom": "^15.5.4",
    "ts-loader": "^2.1.0",
    "typescript": "^2.3.4",
    "webpack": "^2.6.1"
  },
  "directories": {
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/D10221/milligrami.git"
  },
  "bugs": {
    "url": "https://github.com/D10221/milligrami/issues"
  },
  "homepage": "https://github.com/D10221/milligrami#readme"
}
